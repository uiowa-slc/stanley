{
    "name": "silverstripe/installer",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": ">=5.3.2",
        "silverstripe/framework": "4.9.*@stable",
        "silverstripe/cms": "4.9.*@stable",
        "silverstripe/recipe-plugin": "*",
        "silverstripe/recipe-cms": "*",
        "silverstripe/reports": "*",
        "silverstripe/siteconfig": "*",
        "silverstripe-themes/simple": "*",
        "md/saml": "dev-md",
        "md/division-analytics": "1.3.*",
        "md/uiowa-bar": "2.*",
        "silverstripe/blog": "*",
        "md/ac-json-events": "*",
        "silverstripe/mathspamprotection": "*",
        "jonom/focuspoint": "*",
        "silverstripe/userforms": "*",
        "silverstripe/googlesitemaps": "*",
        "colymba/gridfield-bulk-editing-tools": "*",
        "undefinedoffset/sortablegridfield": "*",
        "silverstripe/widgets": "*",
        "silverstripe/lumberjack": "*",
        "undefinedoffset/silverstripe-nocaptcha": "*"
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "resources-dir": "_resources",
        "expose": [
            "themes/stanley/css",
            "themes/stanley/fonts",
            "themes/stanley/images",
            "themes/stanley/js",
            "themes/stanley/lib"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/ac-json-events"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/division-analytics"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/silverstripe-saml"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/uiowa-bar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/StudentLifeMarketingAndDesign/deploy-helper"
        }
    ]
}
